{
    "manifest_version": 3,
    "name": "StockStar Extension",
    "version": "1.0",
    "description": "A Chrome extension for displaying stock data and managing cookies.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "storage",
      "cookies",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://www.alphavantage.co/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "options_page": "options.html",
    "web_accessible_resources": [
      {
        "resources": ["cookies.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  